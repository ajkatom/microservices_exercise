ce":"components/Root/Login/Login.js","original":{"line":72,"column":29}},{"generated":{"line":151,"column":26},"source":"components/Root/Login/Login.js","original":{"line":72,"column":6}},{"generated":{"line":152,"column":0},"source":"components/Root/Login/Login.js","original":{"line":72,"column":43}},{"generated":{"line":152,"column":4},"source":"components/Root/Login/Login.js","original":{"line":72,"column":43}},{"generated":{"line":152,"column":8},"source":"components/Root/Login/Login.js","original":{"line":72,"column":47}},{"generated":{"line":152,"column":10},"source":"components/Root/Login/Login.js","original":{"line":72,"column":48}},{"generated":{"line":153,"column":0},"source":"components/Root/Login/Login.js","original":{"line":72,"column":6}},{"generated":{"line":153,"column":13},"source":"components/Root/Login/Login.js","original":{"line":63,"column":4}},{"generated":{"line":153,"column":14},"source":"components/Root/Login/Login.js","original":{"line":62,"column":2}},{"generated":{"line":154,"column":0},"source":"components/Root/Login/Login.js","original":{"line":77,"column":1}},{"generated":{"line":154,"column":1},"source":"components/Root/Login/Login.js","original":{"line":42,"column":0}},{"name":"Login","generated":{"line":156,"column":15},"source":"components/Root/Login/Login.js","original":{"line":79,"column":15}}],"sources":{"components/Root/Login/Login.js":"import React from 'react';\nimport { useDispatch } from 'react-redux';\nimport { useForm } from 'react-hook-form';\nimport { useMutation } from '@apollo/react-hooks';\nimport gql from 'graphql-tag';\nimport styled from 'styled-components';\n\nimport TextIntput from '#root/components/Root/sharedComponents/TextInput';\nimport { setSession } from '../../../../redux/ducks/session';\n\nconst Label = styled.label`\n  display: block;\n\n  :not(:first-child) {\n    margin-top: 0.75rem;\n  }\n`;\n\nconst LabelText = styled.strong`\n  display: block;\n  font-size: 0.9rem;\n  margin-bottom: 0.25rem;\n`;\n\nconst LoginButton = styled.button`\n  display: inline-block;\n  margin-top: 0.5rem;\n`;\n\nconst mutation = gql`\n  mutation($email: String!, $password: String!) {\n    createUserSession(email: $email, password: $password) {\n      id\n      user {\n        id\n        email\n      }\n    }\n  }\n`;\n\nconst Login = () => {\n  const dispatch = useDispatch();\n  const [createUserSession] = useMutation(mutation);\n  const {\n    formState: { isSubmitting },\n    handleSubmit,\n    register,\n  } = useForm();\n\n  const onSubmit = handleSubmit(async ({ email, password }) => {\n    const {\n      data: { createUserSession: newSession },\n    } = await createUserSession({\n      variables: {\n        email,\n        password,\n      },\n    });\n    dispatch(setSession(newSession));\n  });\n  return (\n    <form onSubmit={onSubmit}>\n      <Label>\n        <LabelText>Email</LabelText>\n        <TextIntput disabled={isSubmitting} name=\"email\" type=\"email\" ref={register} />\n      </Label>\n      <Label>\n        <LabelText>Password</LabelText>\n        <TextIntput disabled={isSubmitting} name=\"password\" type=\"password\" ref={register} />\n      </Label>\n      <LoginButton disabled={isSubmitting} type=\"submit\">\n        Login\n      </LoginButton>\n    </form>\n  );\n};\n\nexport default Login;\n"},"lineCount":null}},"error":null,"hash":"463b37aa422dbd3decc4b098b4b15826","cacheData":{"env":{}}}